%h2 New Wine
= form_for @wine do |f|
    %p
        = f.label :year
        = f.text_field :year
    
    %p
        = f.label :winery
        = f.text_field :winery
    
    %p
        = f.label :wine
        = f.text_field :wine
    
    %p
        = f.label :description
        = f.text_area :description
    
    %p
        = f.submit
